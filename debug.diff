+import React from 'react'
+import { IS_BROWSER, DISABLE_SPEEDY, SC_ATTR } from '../constants'
+import { type ExtractedComp } from '../utils/extractCompsFromCSS'
+import { splitByRules } from '../utils/stringifyRules'
